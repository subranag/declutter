{
  "name": "declutter",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "config": {
    "appName": "declutter"
  },
  "scripts": {
    "build": "bun run clean && bun run build:all",
    "build:all": "bun run build:windows && bun run build:mac && bun run build:mac:arm && bun run build:linux && bun run build:linux:arm",
    "build:windows": "bun build --compile --minify ./src/index.ts --outfile ./dist/$npm_package_config_appName-windows-x64.exe --target=bun-windows-x64",
    "build:mac": "bun build --compile --minify ./src/index.ts --outfile ./dist/$npm_package_config_appName-macos-x64 --target=bun-darwin-x64",
    "build:mac:arm": "bun build --compile --minify ./src/index.ts --outfile ./dist/$npm_package_config_appName-macos-arm64 --target=bun-darwin-arm64",
    "build:linux": "bun build --compile --minify ./src/index.ts --outfile ./dist/$npm_package_config_appName-linux-x64 --target=bun-linux-x64",
    "build:linux:arm": "bun build --compile --minify ./src/index.ts --outfile ./dist/$npm_package_config_appName-linux-arm64 --target=bun-linux-arm64",
    "build:current": "bun build --compile --minify ./src/index.ts --outfile ./dist/app",
    "clean": "rm -rf ./dist && rm -f ./.*.bun-build",
    "postbuild": "chmod +x ./dist/$npm_package_config_appName* && rm -f ./.*.bun-build",
    "dev": "bun run --watch ./src/index.ts",
    "start": "bun run ./src/index.ts"
  },
  "devDependencies": {
    "@types/bun": "1.3.5",
    "@types/puppeteer": "^7.0.4",
    "@types/turndown": "^5.0.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "ai": "^6.0.3",
    "ai-sdk-ollama": "^3.0.1",
    "cmd-ts": "^0.14.3",
    "highlight.js": "^11.11.1",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.3",
    "puppeteer": "^24.34.0",
    "turndown": "^7.2.2",
    "zod": "^4.2.1"
  }
}